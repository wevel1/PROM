<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.dm2.ejercicios.layout5">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/l5_but1"
        android:text="YAHOO"
        android:onClick="Yahoo"/>

    <Button
        android:layout_marginLeft="50dp"
        android:layout_toRightOf="@id/l5_but1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/l5_but2"
        android:text="BING"/>

    <Button
        android:layout_marginLeft="50dp"
        android:layout_toRightOf="@id/l5_but2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/l5_but3"
        android:text="GOOGLE"/>

    <ImageView
        android:id="@+id/l5_img"
        android:adjustViewBounds="false"
        android:layout_marginTop="150dp"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_toRightOf="@id/l5_but1"
        android:layout_below="@id/l5_but2"/>



</RelativeLayout>
